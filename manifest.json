{
    "name": "show-alt",
    "version": "1.0",
    "description": "Show alt texts of images visually",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["style.css"]
      }
    ],
    "manifest_version": 2,
    "permissions": [
      "tabs",
      "activeTab",
      "webNavigation"
    ]
  }